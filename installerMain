sleep(0.5)
print("")
print("DDE Version 0.5")
print("")
 
sleep(0.5)
 
function dlComplete(name)
	write(name)
	print(" File Downloaded Successfully")
end
 
local r = http.get("https://raw.githubusercontent.com/DemApplez/DDE/main/.start")
local f = fs.open(shell.resolve(".start"),"w")
f.write(r.readAll())
f.close()
r.close()
 
dlComplete(".start")
 
 
