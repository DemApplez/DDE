term.clear()
term.setTextColor(colors.black)
  term.setBackgroundColor(colors.white)
local w,h = term.getSize()

--Print On Center Function

function printCenter(y,s)
	local x = math.floor((w - string.len(s)) /2)
	term.setCursorPos(x,y)
	term.clearLine()
	term.write(s)
end
